<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>下载页</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 20px;
            background-color: #f4f4f4;
        }

        .container {
            max-width: 800px;
            margin: auto;
            background: #fff;
            padding: 20px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        }

        .file-list {
            margin-bottom: 20px;
        }

        h1, h2 {
            color: #333;
        }

        ul {
            list-style-type: none;
            padding: 0;
        }

        li {
            background: #eee;
            padding: 10px;
            margin-bottom: 5px;
        }

        a {
            color: #007BFF;
            text-decoration: none;
        }

        a:hover {
            text-decoration: underline;
        }
    </style>
</head>
<body>
    <div class="container">
        <h1>下载页</h1>
        <div class="file-list">
            <h2>文件列表</h2>
            <ul id="fileList"></ul>
        </div>
    </div>
    <script>
        // 定义文件数组
        const files = [
            { name: "[中文译名修正]Minecraft 1.21.51.02 armv8 release.apk", link: "https://ghgo.top/https://github.com/masgzy/zy/releases/download/large/Minecraft.1.21.51.02.armv8.release.apk" },
            { name: "", link: "" }
        ];

        // 动态生成文件列表
        function generateFileList() {
            const fileList = document.getElementById('fileList');
            files.forEach(file => {
                const li = document.createElement('li');
                li.innerHTML = `${file.name} - <a href="${file.link}" target="_blank">下载</a>`;
                fileList.appendChild(li);
            });
        }

        // 页面加载完成后生成文件列表
        window.onload = generateFileList;
    </script>
</body>
</html>